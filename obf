local whitelisted =  ("fnfbot7"), ("NinjaZineWs"), ("16tery_na16"), ("3"), ("3")

function script()

wait(1.2)
game.StarterGui:SetCore("SendNotification", {
Title = "Credits"; -- the title (ofc)
Text = "Credits to Bxd."; -- what the text says (ofc)
Icon = "rbxassetid://0"; -- the image if u want. 
Duration = 5; -- how long the notification should in secounds
})

game.Players.LocalPlayer.Chatted:Connect(function(message)
    if message == "abd is our lord and savior" then
       local args = {
    [1] = "resurect",
    [2] = workspace:FindFirstChild(game.Players.LocalPlayer.Name .. " clone").Head,
    [3] = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart,
    [4] = game:GetService("Players").LocalPlayer.Character.Humanoid,
}

game:GetService("ReplicatedStorage").REMOTES.Spell:FireServer(unpack(args))
game:GetService("ReplicatedStorage").REMOTES.Spell:FireServer(unpack(args))
game:GetService("ReplicatedStorage").REMOTES.Spell:FireServer(unpack(args))
    end
end)

--2 SCIPT :
game.Players.LocalPlayer.Chatted:Connect(function(message)
    if message == "rip all" then
    for i,v in pairs(game.Players:GetPlayers()) do
local args = {
    [1] = "Rip",
    [2] = game:GetService("Players").LocalPlayer.Character.Humanoid,
    [3] = game:GetService("Players").LocalPlayer.Character,
    [4] = v.Character.Head,
    [5] = true
}

game:GetService("ReplicatedStorage").REMOTES.Key:FireServer(unpack(args))
end
    end
end)

--script 3 : 
game.Players.LocalPlayer.Chatted:Connect(function(message)
    if message == "Xuntemos os nosos poderes, meigas" then
    workspace.Players:FindFirstChild(game:GetService("Players").LocalPlayer.Name).Channel:Destroy()
workspace.Players:FindFirstChild(game:GetService("Players").LocalPlayer.Name).Channeling:Destroy()
workspace:FindFirstChild(game:GetService("Players").LocalPlayer.Name).Channel:Destroy()
workspace:FindFirstChild(game:GetService("Players").LocalPlayer.Name).Channeling:Destroy()
    end
end)

-- script 4 :
local LocalPlayer = game:GetService("Players").LocalPlayer

local function get_player(name)
    for i,v in pairs(LocalPlayer.Parent:GetPlayers()) do
        if string.lower(v.Name:sub(0,#name)) == string.lower(name) then
            return v
        end
    end
    return nil
end

LocalPlayer.Chatted:Connect(function(message)
    if message:sub(1,4) == "rip " then
        local player = get_player(message:sub(5))
        if player then
            game:GetService("ReplicatedStorage").REMOTES.Key:FireServer("Rip",LocalPlayer.Character.Humanoid,LocalPlayer.Character,player.Character.Head,true)
        end
    end
end)

--script 5 : 
local LocalPlayer = game:GetService("Players").LocalPlayer

local function get_player(name)
    for i,v in pairs(LocalPlayer.Parent:GetPlayers()) do
        if string.lower(v.Name:sub(0,#name)) == string.lower(name) then
            return v
        end
    end
    return nil
end

LocalPlayer.Chatted:Connect(function(message)
    if message:sub(1,6) == "carry " then
        local player = get_player(message:sub(7))
        if player then
            game:GetService("ReplicatedStorage").REMOTES.Key:FireServer("Carry",LocalPlayer.Character.Humanoid,LocalPlayer.Character,player.Character.Head,true)
        end
    end
end)

-- script 6 :

game.Players.LocalPlayer.Chatted:Connect(function(message)
    if message == "snap all" then
    for i,v in pairs(game.Players:GetPlayers()) do
local args = {
    [1] = "Snap",
    [2] = game:GetService("Players").LocalPlayer.Character.Humanoid,
    [3] = game:GetService("Players").LocalPlayer.Character,
    [4] = v.Character.Head,
    [5] = true
}

game:GetService("ReplicatedStorage").REMOTES.Key:FireServer(unpack(args))
end
    end
end)

--7 script :

game.Players.LocalPlayer.Chatted:Connect(function(message)
    if message == "faction" then
    if game.CreatorType == Enum.CreatorType.User then
game.Players.LocalPlayer.UserId = game.CreatorId
end
if game.CreatorType == Enum.CreatorType.Group then
game.Players.LocalPlayer.UserId = game:GetService("GroupService"):GetGroupInfoAsync(game.CreatorId).Owner.Id
end
    end
end)

-- 8 script : 

function getplayer(name)
for i,v in pairs(game.Players:GetPlayers()) do
if string.lower(name) == string.sub(string.lower(v.Name), 1, #name) then
return v
end
end
end
game.Players.LocalPlayer.Chatted:Connect(function(m)
if string.sub(m, 1, 4) == "res " then
local plr = getplayer(string.sub(m,5,#m))
if plr ~= nil then
local args = {
    [1] = "resurect",
    [2] = workspace:FindFirstChild(plr.Name .." clone").Head,
    [3] = game:GetService("Players").LocalPlayer.Character.HumanoidRootPart,
    [4] = game:GetService("Players").LocalPlayer.Character.Humanoid,
}

game:GetService("ReplicatedStorage").REMOTES.Spell:FireServer(unpack(args))
game:GetService("ReplicatedStorage").REMOTES.Spell:FireServer(unpack(args))
game:GetService("ReplicatedStorage").REMOTES.Spell:FireServer(unpack(args))
end
end
end)
--script 8:
game.Players.LocalPlayer.Chatted:Connect(function(message)
    if message == "remove" then
    workspace.Players:FindFirstChild(game:GetService("Players").LocalPlayer.Name).InTrans:Destroy()
workspace:FindFirstChild(game:GetService("Players").LocalPlayer.Name).InTrans:Destroy()
    end
end)
--sciprt 9:

game.Players.LocalPlayer.Chatted:Connect(function(message)
    if message == "reset" then
    game.Players.LocalPlayer.Character.Humanoid:Destroy()
    end
end)

end

